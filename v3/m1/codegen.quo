import 'ast';
import 'parser';

fn codegen(sourceFilePath: String) {
  let source: String = ReadFile(sourceFilePath);
  if (source == null) {
    Print("Failed to open ");
    Print(sourceFilePath);
    return;
  }
  Print(source);
}

fn Main(argv: Array<String>): Int64 {
  let i: Int64 = 1;
  while (i < argv.length()) {
    codegen(argv[i]);
    i = i + 1;
  }
  return 0;
}
